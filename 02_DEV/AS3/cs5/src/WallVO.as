package {		import flash.geom.Rectangle;	import flash.display.MovieClip;		public class WallVO {				public var id:int;		public var asset_mc:MovieClip		public var dim_rect:Rectangle;		public var friction_amt:Number;		public var bounce_amt:Number;						public function WallVO(i:int, asset:MovieClip, dim:Rectangle, frict:Number, bounce:Number) {			id = i;			asset_mc = asset;			dim_rect = new Rectangle(dim.x, dim.y, dim.width, dim.height);			friction_amt = frict;			bounce_amt = bounce;		}	}	}