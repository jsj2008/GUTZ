<?xml version="1.0" encoding="utf-8"?>

<!-- -=[ START OF BODY PART UI ]=- -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=- -->
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx"
		 
	creationComplete="uponCreation(event)" 
	addedToStage="uponAdoption(event)">
	<!-- ////////////////////////////////////////////////// -->
	
	<!-- -=[ CSS STYLE SHEET ]=- -->
	<fx:Style source="./../assets/styles/default.css" />
	<!-- /////////////////////////////// -->
	
	
	<!-- -=[ MXML SCRIPT ]=- -->
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			import mx.graphics.SolidColor;
			import mx.graphics.SolidColorStroke;
			//]~=~=~=~=~=~=~=~=~=~=~=~=~=~[]~=~=~=~=~=~=~=~=~=~=~=~=~=~[
			
			
			//] APP PROPERTIES ]>
			//]=~=~=~=~=~=~=~=~=~=~=~=~=~=~=~.
			public var pos_pt:Point;
			public var radius:int;
			public var type_str;
			public var ind:int;
			
			public var stroke_color:SolidColorStroke;
			public var fill_color:SolidColor;
			//[¡]~=~=~=~=~=~=~=~=~=~=~=~=~=~[!]~=~=~=~=~=~=~=~=~=~=~=~=~=~[¡].
			
			
			
			//* *\[ APP EVENT HANDLERS ]/*
			//*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*\
			
			
			
			private function uponCreation(e:FlexEvent):void {
			//~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~~*~._
				trace ("[:_:] "+this+".uponCreation() [:_:]");
			}//]~*~~*~~*~~*~~*~~*~~*~~*~~·¯


			private function uponAdoption(e:Event):void {
			//~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~~*~._
				trace ("[:_:] "+this+".uponAdoption() [:_:]");
			}//]~*~~*~~*~~*~~*~~*~~*~~*~~·¯

			
			
		//*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~·¯
		//*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*~~*·¯
		]]>
	</fx:Script>
	
	
	<!-- -=[ FX OBJS ]=- -->
	<fx:Declarations />
	<!-- /////////////////////////////// -->
	
	
	
	<!-- -=[ OUTPUT UI ]=- -->
	<!-- =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-= -->
	<s:Ellipse width="{radius * 2}" height="{radius * 2}"
	<!-- /////////////////////////////// -->
	
</s:Group>
<!-- -=[ END OF BODY PART UI ]=- -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=- -->
