package com.sparklemountain.gutz.creaturebuilder.model.vo  {		import flash.geom.Point;		import mx.core.IVisualElement;		import spark.primitives.Line;
		public class JointVO extends AbstractPartVO implements iPartVO {				public var bodyA:int;		public var bodyB:int;		public var offsetA_pt:Point;		public var offsetB_pt:Point;		public var str:Number;		public var dmp:Number;				public function JointVO(idx:int, kind:String, body1:int, body2:int, offset1:Point, offset2:Point, s:Number, d:Number, asset:IVisualElement=null) {			super(idx, kind, new Point(), asset);						bodyA = body1;			bodyB = body2;						offsetA_pt = new Point(offset1.x, offset1.y);			offsetB_pt = new Point(offset2.x, offset2.y);						str = s;			dmp = d;		}	}}